  "authors": [
    "tjatse"
  ],
  "license": "MIT",
  "keywords": [
    "monitor",
    "dashboard",
    "PM2",
    "PM2",
    "interface",
    "PM2",
    "web",
    "PM2",
    "ui",
    "PM2",
    "gui",
    "PM2",
    "dashboard",
    "PM2",
    "monitor"
  ],
  "homepage": "https://github.com/Tjatse/pm2-gui",
